{
    "version": 2,
    "name": "my-backend-project",
    "builds": [
      {
        "src": "src/app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
        { "handle": "vercel", "src": "/(.*)", "dest": "src/app.js" }
      ],      
      "env": {
        "JWT_SECRET": "Secret_Token"
      }
      
  }
    