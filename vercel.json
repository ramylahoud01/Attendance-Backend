{
  "version": 2,
  "name": "my-backend-project",
  "builds": [
    {
      "src": "src/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "src/app.js" }
  ],
  "functions": {
    "/Employee/new": {
      "maxDuration": 30 
    }
  },
  "env": {
    "JWT_SECRET": "Secret_Token"
  }
}
